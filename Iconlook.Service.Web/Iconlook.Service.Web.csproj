<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <Version>0.0.0.0</Version>
    <Authors>Tom Nuen</Authors>
    <OutputType>Exe</OutputType>
    <LangVersion>preview</LangVersion>
    <TieredCompilation>true</TieredCompilation>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Copyright>Copyright © ICONVIET 2018-2019</Copyright>
    <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="wwwroot\images\p-reps\**" />
    <Content Remove="wwwroot\images\p-reps\**" />
    <EmbeddedResource Remove="wwwroot\images\p-reps\**" />
    <None Remove="wwwroot\images\p-reps\**" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Dockerfile">
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Update="appconfig.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Iconlook.Service.Api\Iconlook.Service.Api.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="BuildBundlerMinifier" Version="2.9.406" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Protocols.MessagePack" Version="3.0.0-preview7.19365.7" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.StackExchangeRedis" Version="3.0.0-preview7.19365.7" />
    <PackageReference Include="Syncfusion.EJ2.Blazor" Version="17.2.0.40-beta" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.2.0.40" />
    <PackageReference Include="WebMarkupMin.AspNetCore3" Version="2.6.0-preview1" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="wwwroot\fonts\" />
    <Folder Include="wwwroot\js\" />
  </ItemGroup>
</Project>
