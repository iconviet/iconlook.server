<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <Version>0.0.0.0</Version>
    <Authors>Tom Nuen</Authors>
    <OutputType>Exe</OutputType>
    <LangVersion>preview</LangVersion>
    <TieredCompilation>true</TieredCompilation>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Copyright>Copyright © ICONVIET 2018-2019</Copyright>
    <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="Dockerfile">
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Update="appconfig.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Iconlook.Service.Api\Iconlook.Service.Api.csproj" />
    <ProjectReference Include="..\Iconlook.Service\Iconlook.Service.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.EJ2.Blazor" Version="17.2.0.35-beta" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.2.0.39" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="wwwroot\fonts\" />
  </ItemGroup>
</Project>
